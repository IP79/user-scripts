{
  "name": "user-scripts",
  "version": "1.0.0",
  "description": "User scripts for Tampermonkey",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint:js && npm run jrdb:test && npm run ple:test",
    "ple:test": "rollup --config rollup.config.ple.js",
    "ple:build": "npm run ple:test && npm run lint:dist",
    "ple:wip:build": "rollup --config rollup.config.ple.wip.js && npm run lint:dist",
    "jrdb:test": "rollup --config rollup.config.jrdb.js",
    "jrdb:build": "npm run jrdb:test && npm run lint:dist",
    "lint:dist": "eslint --fix dist/*",
    "lint:js": "eslint src/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shikiyoku/user-scripts.git"
  },
  "keywords": [
    "user.js",
    "userscript"
  ],
  "author": "shikiyoku",
  "license": "MIT",
  "devDependencies": {
    "blissfuljs": "^1.0.3",
    "eslint": "^4.12.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "rollup": "^0.52.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-userscript-metablock": "^0.1.0",
    "rollup-pluginutils": "^2.0.1",
    "stylelint": "^8.3.0",
    "stylelint-config-property-sort-order-smacss": "^2.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.7.0"
  },
  "dependencies": {}
}
